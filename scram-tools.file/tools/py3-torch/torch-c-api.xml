<tool name="torch-c-api" version="@TOOL_VERSION@">
  <lib name="npymath"/>
  <client>
    <environment name="TORCH_C_API_BASE" default="@TOOL_ROOT@"/>
    <environment name="INCLUDE" default="$TORCH_C_API_BASE/c-api/include"/>
    <environment name="LIBDIR" default="$TORCH_C_API_BASE/c-api/lib"/>
  </client>
  <runtime name="ROOT_INCLUDE_PATH" value="$INCLUDE" type="path"/>
  <use name="root_cxxdefaults"/>
</tool>
